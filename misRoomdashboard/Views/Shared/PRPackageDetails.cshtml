@model Rooms.Models.BookedDatesCustomized
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>personal details</title>
</head>
<body>
    <hr />   
    <center><h2 style="font-weight:bold;font-size:x-large"><u>Package Details</u></h2></center>
    <table cellpadding="0" cellspacing="0" width="100%" style="border: 2px;" rules="none">
        <tr>
            <td style="font-weight:bold">Package Type</td>
            <td>: @Html.DisplayFor(model => model.PackageType)</td>
            <td style="font-weight:bold">Package Name</td>
            <td>: @Html.DisplayFor(model => model.PackageName)</td>
        </tr>
        <tr>
            <td style="font-weight:bold">No of Days</td>
            <td>: @Html.DisplayFor(model => model.NoOfDays)</td>
            @*<td colspan="2"></td>*@
            <td style="font-weight:bold">Arrival Date</td>
            <td>: @Html.DisplayFor(model => model.ArrivalDate)</td>
        </tr>
       <tr>           
            <td style="font-weight:bold">Departure Date</td>
            <td>: @Html.DisplayFor(model => model.DepartureDate)</td>
            <td style="font-weight:bold"> Alternate Arrival Date</td>
            <td>: @Html.DisplayFor(model => model.AlternateArrivalDate)</td>
        </tr>
        <tr>         
            <td style="font-weight:bold">Alternate Departure Date</td>
            <td>: @Html.DisplayFor(model => model.AlternateDepartureDate)</td>
            <td style="font-weight:bold"> Room View</td>
            <td>: @Html.DisplayFor(model => model.RoomView)</td>
        </tr>
        <tr>           
            <td style="font-weight:bold">Room Type</td>
            <td>: @Html.DisplayFor(model => model.RoomType)</td>
            <td style="font-weight:bold">AccompanyingGuests</td>
            <td>: @Html.DisplayFor(model => model.AccompanyingGuests)</td>
        </tr>
        <tr>         
            <td style="font-weight:bold">Tariff Amount</td>
            <td>: @Html.DisplayFor(model => model.RoomTariff)</td>
        </tr>
    </table>      
</body>
</html>
