<script>
    function dateconversion(date, ctrlid) {
        debugger;
        alert(moment(date).format("DD/MM/YYYY"));
        if (date != null) {
            var pattern = /Date\(([^)]+)\)/;
            var results = pattern.exec(date);
            var IST = new Date(parseFloat(results[1])); // Clone UTC Timestamp
            IST.setHours(IST.getHours() + 5); // set Hours to 5 hours later
            IST.setMinutes(IST.getMinutes() + 30); // set Minutes to be 30 minutes later
            //alert(IST);
            //debugger;
            document.getElementById(ctrlid).value = new Date(IST);

        }
    }
    function myfunctionitem(data) {
        debugger;
        if (data.value != "") {
            var url = '@Url.Action("GetGuestdetails", "Checkin")';
            item = data.value;
            var postData = {
                prno: item
            };
            $.ajax({
                url: url,
                type: 'POST',
                data: postData,
                success: function (result) {
                    debugger;
                    if (result != "none") {
                        $('#fname').val(result[0].a.FirstName);
                        $('#lname').val(result[0].a.LastName);
                        $("#mbl").val(result[0].a.MobileNo);
                        $("#age").val(result[0].a.Age);
                        $("#flat").val(result[0].a.FlatNo);
                        $("#build").val(result[0].a.Building);
                        $("#street").val(result[0].a.Street);
                        $("#city").val(result[0].a.City);
                        //$("#NationalityDrop").val(result[0].a.Nationality).change();
                        //$('#NationalityDrop').selectmenu('refresh').val(result[0].a.Nationality).attr("s‌​elected", "selected");
                        $("#NationalityDrop").val(result[0].a.Nationality);
                        $("#CountryDrop").val(result[0].a.Country);
                        $("#Gender").val(result[0].a.Gender);
                        $("#email").val(result[0].a.EmailId);
                        $("#PackageTypeDrop").val(result[0].b.PackageType);
                        //$("#PackageNameDrop1").append('<option value="' + result[0].b.PackageName + '">' + result[0].b.PackageName + '</option>');
                        $("#PackageNameDrop").val(result[0].b.PackageName);
                        $("#RoomviewDrop").val(result[0].b.RoomView);
                        $("#RoomTypeDrop").val(result[0].b.RoomType);
                        //dateconversion(result[0].b.ArrivalDate,'arrivaldate')
                        $("#arrivaldate").val(moment(result[0].b.ArrivalDate).format("DD/MM/YYYY"));
                        $("#departuredate").val(moment(result[0].b.DepartureDate).format("DD/MM/YYYY"));
                        $("#alternatearrivaldate").val(moment(result[0].b.AlternateArrivalDate).format("DD/MM/YYYY"));
                        $("#alternatedepartureldate").val(moment(result[0].b.AlternateDepartureDate).format("DD/MM/YYYY"));
                        $("#Noofdays").val(result[0].b.NoOfDays);
                        $("#PackageAmount").val(result[0].b.Package_Amount);
                        if (result[0].b.Accompany_Type == null) {
                            $("#AccompanyType").val('-1');
                        } else {
                            $("#AccompanyType").val(result[0].b.Accompany_Type);
                        }
                    } else {
                        alert('PRNO doesn"t Exists');
                    }
                }
            });
        }
        else {
            alert('enter PRNO');
        }
    }
</script>
